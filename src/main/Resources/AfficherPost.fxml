<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.media.MediaView?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tn.esprit.controllers.AfficherPost">
   <children>
      <MediaView fx:id="mediaView" fitHeight="300.0" fitWidth="600.0" layoutX="3.0" />
      <Button fx:id="buttonPLay" layoutX="149.0" layoutY="311.0" mnemonicParsing="false" onAction="#playMedia" stylesheets="@Test.css" text="PLAY" textFill="WHITE" />
      <Button fx:id="buttonPause" layoutX="268.0" layoutY="311.0" mnemonicParsing="false" onAction="#pauseMedia" stylesheets="@Test.css" text="PAUSE" textFill="WHITE" />
      <Button fx:id="buttonReset" layoutX="373.0" layoutY="311.0" mnemonicParsing="false" onAction="#resetMedia" stylesheets="@Test.css" text="RESET" textFill="WHITE" />
      <Button layoutX="521.0" layoutY="346.0" mnemonicParsing="false" onAction="#Modifier" stylesheets="@Test.css" text="Modifer" textFill="WHITE" />
      <Slider fx:id="slider" layoutX="2.0" layoutY="285.0" onMousePressed="#seeking" prefHeight="14.0" prefWidth="600.0" stylesheets="@Style.css" />
      <ImageView fx:id="Img" fitHeight="282.0" fitWidth="323.0" layoutX="149.0" layoutY="9.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="31.0" layoutY="346.0" mnemonicParsing="false" onAction="#Supprimer" stylesheets="@Test.css" text="Supprimer" textFill="WHITE" />
      <Button layoutX="230.0" layoutY="346.0" mnemonicParsing="false" onAction="#AjouterCommentaire" stylesheets="@Test.css" text="AjouterCommentaire" textFill="WHITE" />
   </children>
</AnchorPane>
